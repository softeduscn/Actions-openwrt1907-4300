config sysmonitor 'sysmonitor'
	option enable '1'
	option time '10'
	option homeip '192.168.1.1'
	option vpnip '192.168.1.110'
	option ddns '1'
	option smartdns '0'
	option smartdnsPORT '53'
	option smartdnsAD '0'
	option ipsec '0'
	option pptp '0'
	option firmware 'https://github.com/softeduscn/Actions-openwrt1907-4300/releases/download/WNDR-4300/openwrt-ar71xx-nand-wndr4300-squashfs-sysupgrade.tar'
	list dns '119.29.29.29'
	list dns '223.5.5.5'
	list dns '2400:3200::1'
	list vpn '192.168.1.110'
	list vpn '192.168.1.220'

